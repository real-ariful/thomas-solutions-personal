        <record id='thomas_workorder_view_form' model='ir.ui.view'>
            <field name="name">Work Order</field>
            <field name="model">thomasfleet.workorder</field>
            <field name="arch" type="xml">
                <form string="Invoice">
                    <sheet>
                        <group>
                            <field name="vehicle_id"/>
                            <field name="customer_id"/>
                            <field name="product_id"/>
                            <field name="workOrderDate" widget="date"/>
                            <field name="workOrderNumber"/>
                            <field name="invoiceDate" widget="date"/>
                            <field name="invoiceNumber"/>
                            <field name="partsTotal"/>
                            <field name="subletTotal"/>
                            <field name="grandTotal"/>
                            <field name="laborTotal"/>
                            <field name="netTotal"/>
                            <field name="workorder_details" string="Details" nolabel="1" widget="one2many" mode="tree">
                                <tree>
                                    <field name="title"/>
                                    <field name="description"/>
                                    <field name="workorder_line_items" string="Line Items" nolabel="1" widget="one2many">
                                        <tree>
                                            <field name="complete"/>
                                            <field name="rank"/>
                                            <field name="type"/>
                                            <field name="description"/>
                                            <field name="quantity"/>
                                            <field name="unit"/>
                                            <field name="rate_code"/>
                                            <field name="price"/>
                                            <field name="price_unit"/>
                                            <field name="minimum_charge"/>
                                            <field name="total"/>
                                            <field name="discount"/>
                                            <field name="extended_total"/>
                                            <field name="total_cost"/>
                                            <field name="other_charge_code"/>
                                            <field name="tags"/>
                                            <field name="flag"/>
                                            <field name="technician_name"/>
                                            <field name="service_advisor"/>
                                        </tree>
                                    </field>
                                </tree>
                            </field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

