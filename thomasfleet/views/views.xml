        <record id='thomas_asset_photo_kanban' model='ir.ui.view'>
            <field name="name">thomasfleet.asset_photo.kanban</field>
            <field name="model">thomasfleet.asset_photo</field>
            <field name="arch" type="xml">
                <kanban>
                    <field name="photo_set_id"/>
                    <!-- <field name="image"/> -->
                    <field name="id"/>
                    <field name="position"/>
                    <templates>
                        <t t-name="kanban-box">

                            <div class="oe_kanban_global_click">
                                <div class="oe_kanban_card">
                                    <strong class="o_kanban_record_title">
                                        <label for="position"/>
                                        <field name="position"/>
                                    </strong>
                                    <div class="oe_kanban_details">
                                        <div class="o_kanban_image">
                                            <img t-att-src="kanban_image('thomasfleet.asset_photo', 'image_medium', record.id.raw_value)"
                                                 class="img-responsive" alt="Asset Image"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <record id='thomas_asset_photo_set_kanban' model='ir.ui.view'>
            <field name="name">thomasfleet.asset_photo_set.kanban</field>
            <field name="model">thomasfleet.asset_photo_set</field>
            <field name="arch" type="xml">
                <kanban class="o_kanban_mobile">
                    <field name="id"/>
                    <field name="vehicle_id"/>
                    <field name="photoDate"/>
                    <field name="encounter"/>
                    <field name="photos"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_global_click">
                                <div class="oe_kanban_card">
                                    <strong class="o_kanban_record_title">
                                        Encounter Type
                                        <field name="encounter"/>
                                        <br/>
                                        Date
                                        <field name="photoDate"/>
                                    </strong>
                                    <div class="oe_kanban_details">
                                        <t t-foreach="record.photos.raw_value" t-as="i">
                                            <img t-att-src="kanban_image('thomasfleet.asset_photo', 'image_medium', i)" alt="Asset Image"/>
                                        </t>
                                    </div>

                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

